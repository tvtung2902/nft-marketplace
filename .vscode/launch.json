{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Next.js Debug",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "node",
        "args": ["node_modules/next/dist/bin/next", "dev"],
        "cwd": "${workspaceFolder}",
        "autoAttachChildProcesses": true,
        "sourceMaps": true,
        "smartStep": true,
        "protocol": "inspector",
        "console": "integratedTerminal"
      }
    ]
  }
  